{
    "name": "javaabu/mediapicker",
    "description": "Allow for file uploads from a persistent storage",
    "homepage": "https://github.com/Javaabu/mediapicker",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "mediapicker"
    ],
    "authors": [
        {
            "name": "FlameXode",
            "email": "wovencoast14782@techie.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.5 || ^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Mediapicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Mediapicker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Mediapicker\\MediapickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
