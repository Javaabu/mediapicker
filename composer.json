{
    "name": "javaabu/mediapicker",
    "description": "Allow for file uploads from a persistent storage",
    "homepage": "https://github.com/Javaabu/mediapicker",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "mediapicker"
    ],
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com",
            "role": "Developer"
        },
        {
            "name": "FlameXode",
            "email": "wovencoast14782@techie.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.2 || ^11.0 || ^12.0",
        "spatie/laravel-medialibrary": "^11.12",
        "javaabu/helpers": "^1.65",
        "javaabu/activitylog": "^1.4",
        "javaabu/forms": "^1.38"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.0 || ^9.13 || ^10.0",
        "phpunit/phpunit": "^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Mediapicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Mediapicker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Mediapicker\\MediapickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
